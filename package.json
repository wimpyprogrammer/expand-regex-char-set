{
  "name": "regex-to-strings",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wimpyprogrammer/regex-to-strings.git"
  },
  "bugs": {
    "url": "https://github.com/wimpyprogrammer/regex-to-strings/issues"
  },
  "homepage": "https://github.com/wimpyprogrammer/regex-to-strings#readme",
  "description": "Expand a regular expression pattern into a set of words",
  "author": "Drew Keller <drew@wimpyprogrammer.com>",
  "license": "MIT",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "npm run clean && babel src -d lib --extensions '.js,.ts'",
    "build-demo": "npm run clean-demo && webpack --config demo/webpack.config.js",
    "clean": "rimraf lib/*",
    "clean-demo": "rimraf demo/lib/*",
    "format": "prettier --write '{src,demo/src}/**'",
    "lint": "tsc -p ./tsconfig.release.json; eslint . --ext .js,.ts --parser-options=project:./tsconfig.release.json",
    "lint-demo": "tsc -p ./demo/tsconfig.json; eslint . --ext .js,.ts --parser-options=project:./demo/tsconfig.json",
    "lint-tests": "tsc -p ./tsconfig.test.json; eslint . --ext .js,.ts --parser-options=project:./tsconfig.test.json",
    "precommit": "pretty-quick --staged",
    "test": "jest --coverage",
    "travisci": "npm run lint && npm run test && codecov"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@types/escape-string-regexp": "^1.0.0",
    "@types/jest": "^24.0.13",
    "@types/jest-when": "^2.4.1",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "codecov": "^3.3.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.0",
    "husky": "0.14.3",
    "jest": "^24.8.0",
    "jest-when": "^2.5.0",
    "prettier": "1.14.3",
    "pretty-quick": "1.7.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.4.5",
    "webpack": "^4.38.0",
    "webpack-command": "^0.4.2"
  },
  "dependencies": {
    "escape-string-regexp": "^1.0.5",
    "pandemonium": "^1.4.1",
    "regexp-tree": "^0.1.5"
  }
}
