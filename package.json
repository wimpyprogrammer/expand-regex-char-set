{
  "name": "regex-to-strings",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wimpyprogrammer/regex-to-strings.git"
  },
  "bugs": {
    "url": "https://github.com/wimpyprogrammer/regex-to-strings/issues"
  },
  "homepage": "https://github.com/wimpyprogrammer/regex-to-strings#readme",
  "description": "Expand a regular expression pattern into a set of words",
  "author": "Drew Keller <drew@wimpyprogrammer.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "sideEffects": [
    "demo/**/polyfills.ts",
    "*.scss"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "npm run clean && babel src -d lib --extensions '.js,.ts' --ignore '**/*.spec.js','**/*.spec.ts' && npm run build-types",
    "build-demo": "npm run clean-demo && webpack --config demo/webpack.config.js",
    "build-types": "tsc -p ./tsconfig.release-types.json",
    "clean": "rimraf lib/*",
    "clean-demo": "rimraf demo/lib/*",
    "format": "prettier --write '{src,demo/src}/**'",
    "lint": "tsc -p ./tsconfig.release.json && eslint . --ext .js,.ts --report-unused-disable-directives --parser-options=project:./tsconfig.release.json",
    "lint-demo": "tsc -p ./demo/tsconfig.json && eslint . --ext .js,.ts --report-unused-disable-directives --parser-options=project:./demo/tsconfig.json",
    "lint-tests": "tsc -p ./tsconfig.test.json && eslint . --ext .js,.ts --report-unused-disable-directives --parser-options=project:./tsconfig.test.json",
    "precommit": "pretty-quick --staged",
    "prepublish": "npx publish-please guard",
    "publish-please": "npx publish-please",
    "publish-please-prereqs": "npm run lint && npm run test && npm run build",
    "test": "jest --coverage",
    "travisci": "npm run lint && npm run test && codecov && npx snyk test"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/escape-string-regexp": "^2.0.1",
    "@types/history": "^4.7.3",
    "@types/jest": "^26.0.10",
    "@types/jest-when": "^2.4.1",
    "@types/lodash": "^4.14.140",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "codecov": "^3.5.0",
    "css-loader": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.16.0",
    "husky": "0.14.3",
    "jest": "^26.4.2",
    "jest-when": "^2.7.0",
    "node-sass": "^4.12.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.7.0",
    "rimraf": "^2.7.1",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "ts-jest": "^26.3.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-command": "^0.4.2",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "escape-string-regexp": "^4.0.0",
    "pandemonium": "^1.4.1",
    "regexp-tree": "^0.1.21"
  },
  "snyk": true
}
